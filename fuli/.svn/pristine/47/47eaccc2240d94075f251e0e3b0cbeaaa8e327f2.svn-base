<#include "../inc/layout.html"> 
<@layout title="酒帮修改">
		<div class="common-form">
			<form method="post" class="J_ajaxForm" action="${root!}/jiubang/save">
				<input type="hidden" name="id" value="${(po.id)!}">
				<div class="h_a"> 酒帮基本信息</div>
				<div class="table_list">
					<table cellpadding="0" cellspacing="0" class="table_form" width="100%">
						<tbody>
							<tr>
								<td width="200">酒帮名称:</td>
								<td><input type="text" class="input" name="login" value="${(po.jbname)!}"></td>
							</tr>
						</tbody>
					</table>
				</div>
				<div class="btn_wrap">
					<div class="btn_wrap_pd">
						<button class="btn btn_submit mr10 J_ajax_submit_btn"
							type="submit"><#if (po.id)??>修改<#else>添加</#if></button>
						<a class="btn" href="${root!}/jiubang/list">返回</a>
					</div>
				</div>
			</form>
		</div>
 ·</@layout>