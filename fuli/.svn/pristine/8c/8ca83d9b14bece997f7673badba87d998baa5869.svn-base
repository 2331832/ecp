package net.loyin.util.wechat.bean.mo;

/**
 * 我的产品业务
 *
 * @author EJ-Mr.Von
 */
public class QueryMyProduct {
	private String name;
	private String desc; // 业务介绍
	private int fee; // 业务资费
	private String pic; // 业务图标
	private String code; // 业务编码
	private String status; // 用户订购状态

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}

	public int getFee() {
		return fee;
	}

	public void setFee(int fee) {
		this.fee = fee;
	}

	public String getPic() {
		return pic;
	}

	public void setPic(String pic) {
		this.pic = pic;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

}
