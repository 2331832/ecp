DROP TABLE IF EXISTS `sp_wx_user`;
SET @saved_cs_client     = @@character_set_client;
SET character_set_client = utf8;
CREATE TABLE `sp_wx_user` (
  `id` bigint(20) NOT NULL auto_increment,
  `openid` varchar(50) NOT NULL,
  `jbopenid` varchar(50) default NULL COMMENT '酒帮主openid',
  `jbm` smallint(6) NOT NULL default '0' COMMENT '是否酒帮成员 0，非成员 1成员，2帮主',
  `jbn` smallint(6) NOT NULL default '0' COMMENT '酒帮成员编号',
  `subscribe_time` int(11) default NULL COMMENT '关注时间',
  `nickname` varchar(80) NOT NULL default '',
  `shopcount` int(11) NOT NULL default '0' COMMENT '购买次数',
  `buyct` int(11) NOT NULL default '0',
  `js` smallint(6) NOT NULL default '0' COMMENT '结算标识',
  `sex` smallint(6) default '0',
  `typ` smallint(6) default '0' COMMENT '用户类型 0:消费者，1：经销者',
  `amt` decimal(10,2) NOT NULL default '0.00' COMMENT '购买金额',
  `fanli` decimal(10,2) NOT NULL default '0.00',
  `fanli2` decimal(10,2) NOT NULL default '0.00' COMMENT '计算酒帮奖的返利，当统计后清零',
  `Bonus` decimal(10,2) NOT NULL default '0.00' COMMENT '帮主奖金',
  `Bonus1` decimal(10,2) NOT NULL default '0.00' COMMENT '最近酒帮奖',
  `bonus2` decimal(10,2) NOT NULL default '0.00' COMMENT '预计当期酒帮奖',
  `language` varchar(30) default NULL COMMENT '返利奖金',
  `city` varchar(10) default NULL,
  `uname` varchar(10) default NULL COMMENT '姓名',
  `phone` varchar(20) default NULL COMMENT '手机',
  `province` varchar(30) default NULL,
  `country` varchar(15) default NULL,
  `headimgurl` varchar(200) default NULL,
  `unsubscribe_time` datetime default NULL COMMENT '取消关注时间',
  `status` tinyint(1) default '1',
  `belong` varchar(20) default NULL,
  PRIMARY KEY  (`id`),
  UNIQUE KEY `wxuseropenid_uniq` (`openid`)
) ENGINE=MyISAM AUTO_INCREMENT=135 DEFAULT CHARSET=utf8 COMMENT='微信用户';
SET character_set_client = @saved_cs_client;

