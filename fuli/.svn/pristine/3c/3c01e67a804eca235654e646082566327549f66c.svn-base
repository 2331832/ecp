<#include "../inc/layout.html"> <@layout title="数据库备份">
<div class="nav">
	<button class="btn btn_submit mr10" onclick="backUp();">备份</button>
</div>
<div class="table_list">
	<form name="myform" action="" method="post">
		<table width="100%" cellspacing="0">
			<thead>
				<tr>
					<td width="30">文件</td>
					<td align="left">备份时间</td>
					<td width="200">操作</td>
				</tr>
			</thead>
			<tbody>
				<#list page.list as u>
				<tr>
					<td>${(u.f)!}</td>
					<td>${(u.remark)!}</td>
					<td class="text-c">
					<a class="J_ajax_del" href="${root!}/dbm/del/${u.f}">删除</a></td>
				</tr>
				</#list>
			</tbody>
		</table>
		<div class="p10"><@paginate
			action="" pageNo=pageNo totalPage=page.totalPage!0
			totalCount=page.totalRow!0/></div>
	</form>
</div>
<script>
function backUp(){
	$.ajax({url:'${root!}/dbm/backUp',dataType:'json',success:function(json){
		alert(json.msg);
	}});
}</script>
</@layout>
