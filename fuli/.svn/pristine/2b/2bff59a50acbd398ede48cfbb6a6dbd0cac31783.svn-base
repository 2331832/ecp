@charset "utf-8";
body {margin:0px;padding:0px;font-family: Arial,Helvetica,sans-serif,"宋体";text-align:center;font-size:12px;}
img, a img, object{border:none;vertical-align:middle;}
p, span, ul, li, dt, dd, dl, h1, h2, h3, form, input, figure {margin:0px;padding:0px;list-style:none;}
em {font-style:normal;}
input{border:0 none;}
a{color:#0078B6;text-decoration: none;}
a:hover{color:#0078B6;}
.left{float:left;}
.right{float:right;}
.red{color:#FF0000;}

#header{width:100%;float:left;margin:auto;}
.top{width:100%;height:40px;float:left;background:url(../images/xy_bg.gif);}
.top h1{float:left;}
.search{height:29px;float:right;margin-right:5px;}
.search .an{width:61px;height:29px;float:left;margin-top:5px;font-weight:bold;color:#004A6A;text-align:right;padding-right:10px;display:inline;background:url(../images/search.gif) no-repeat;}
.nav{width:100%;height:39px;float:left;background:#0771BB url(../images/xy_bg.gif) -0 -52px;overflow:hidden;padding-top:1px;margin:auto;}
.nav ul{width:100%;float:left;}
.nav li{width:24%;float:left;color:#E0F8FF;line-height:39px;font-weight:bold;font-size:17px;text-align:center;}
.nav li.nv{width:24%;height:40px;float:left;background:url(../images/xy_bg1.gif);color:#FEFFC2;}
.nav li a{color:#fff;display:block;height:39px;}
.nav li a:hover{color:#fff;}
.nav li.da{width:28%;}
.nav li a:hover{background:url(../images/xy_bg1.gif) -0 -52px;}
.nav li.xian{background:url(../images/xian.gif) no-repeat;}

#contenter{width:100%;float:left;margin:auto;}
.flash{width:100%;float:left;background:#F8F8F8;padding:10px 0;text-align:center;}
.flash p{width:100%;height:25px;float:left;background:url(../images/f_bg.gif);margin-top:10px;}
.flash p img{margin:0 5px;}
.new{width:100%;float:left;height:105px;position:relative;overflow:hidden;}
.new em{width:48px;height:53px;left:0px;top:0px;z-index:5;position:absolute;background:url(../images/new.png);}
.new ul{float:left;line-height:22px;padding:15px 0 10px 0;}
.new li{width:65px;float:left;padding:0 0 10px 12px;}
.new li img{width:65px;height:60px;display:block;margin:auto;}
.list{width:100%;loat:left;}
.tit{width:100%;float:left;height:38px;background:#3496D3 url(../images/l_tit.gif);}
.tit .q1{background:#F2F2F2 url(../images/l_titi.gif);color:#001F34;width:93px;float:left;height:30px;line-height:30px;margin-top:8px;overflow:hidden;margin-left:10px;font-size:14px;font-weight:bold;}
.tit h2{background:#F2F2F2 url(../images/l_titi.gif);color:#001F34;width:93px;float:left;height:30px;line-height:30px;margin-top:8px;overflow:hidden;margin-left:10px;font-size:14px;font-weight:bold;}
.tit h3 {width:93px;float:left;height:30px;line-height:30px;margin-top:8px;overflow:hidden;margin-left:10px;font-size:14px;font-weight:bold;color:#fff;text-shadow:0 2px 0 #0075B2;}
.tit h3 a{color:#fff;text-shadow:0 2px 0 #0075B2;}
.tit .q1 a{color:#001F34;text-shadow:none;}
.tit .right{float:right;line-height:38px;color:#CEF0FF;margin-right:10px;}
.tit .right a{color:#CEF0FF;}
.list ul{width:100%;float:left;}
.list li{width:100%;float:left;text-align:left;border-bottom:1px solid #FFFFFF;}
.list li .figure{width:100%;float:left;padding:5px 0;border-bottom:1px solid #C8D3D9;background:none repeat scroll 0 0 #F2F2F2;}
.list li .name{font-size:16px;font-weight:bold;}
.list li .dl{color:#7AA5B9;}
.list li .dl a{color:#7AA5B9;}
.list li .starbasic{margin-top:2px;}
.list li p{float:left;line-height:23px;color:#575757;overflow:hidden;white-space:nowrap;}
.list .tup{float:left;width:70px;height:70px;margin:0 10px;}
.list .tup img{width:70px;height:70px;}
.list .jr{width:82px;float:right;margin:20px 10px 0 0;}
.more{width:100%;float:left;height:40px;background:url(../images/m_bg.gif);line-height:38px;font-weight:bold;font-size:14px;color:#000;}
.more a{color:#000;height:38px;display:block;}

.page{width:100%;float:left;height:35px;background:url(../images/m_bg.gif);font-size:14px;padding-top:5px;}
.page .left a{color:#fff;line-height:26px;padding:5px 8px;background:#2AB9EB;margin-left:5px;}
.page .right a{color:#00384A;line-height:26px;padding:5px 8px;background:#C2E7F8;margin:0 5px;}
.page .right .tz{color:#00384A;line-height:26px;padding:1px 8px;background:#C2E7F8;margin:0 5px;}
.page .ku{width:30px;height:24px;font-size:14px;border:1px #B3D8E4 solid;text-align:center;margin-left:5px;}

/*详细*/
.dezl{width:100%;float:left;padding:5px 0;text-align: left;}
.dezl .name{font-size:16px;font-weight:bold;}
.dezl .dl{color:#7AA5B9;}
.dezl .dl a{color:#7AA5B9;}
.dezl  p{float:left;line-height:23px;color:#575757;overflow:hidden;white-space:nowrap;}
.dezl .tup{float:left;width:70px;height:70px;margin:0 10px;}
.dezl .tup img{width:70px;height:70px;}
.dezl .jr{width:82px;float:right;margin:20px 10px 0 0;}

.like{width:100%;float:left;height:105px;overflow:hidden;background:#F2F2F2;}
.like ul{float:left;line-height:22px;padding:15px 0 10px 0;}
.like li{width:65px;float:left;padding:0 0 10px 12px;}
.like li img{width:65px;height:60px;display:block;margin:auto;}

.mygame{width:100%;float:left;background:#F2F2F2;}
.mygame ul{float:left;line-height:22px;padding:0 0 10px 0;}
.mygame li{width:65px;float:left;padding:12px 0 0 12px;border:0;}
.mygame li img{width:65px;height:60px;margin:auto;}
.mygame p{text-align:left;line-height:20px;float:left;color:#353535;padding:10px 0 10px 20px;}

/*详情*/
.denr{width:100%;float:left;}
.deqh{width:100%;height:40px;float:left;background:#9ED9F1 url(../images/deta_ti.gif);}
.deqh h2{float:left;width:82px;float:left;height:33px;line-height:33px;margin-top:7px;overflow:hidden;margin-left:10px;font-size:14px;font-weight:bold;color:#005682;background:#fff url(../images/deta_qh.gif) no-repeat;}
.deqh .right{float:right;line-height:38px;color:#2985B4;margin-right:10px;}
.deqh .right a{color:#2985B4;}
.deqh ul{width:100%;height:40px;float:left;}
.deqh li{width:82px;float:left;height:33px;line-height:33px;margin-top:7px;overflow:hidden;margin-left:10px;font-size:14px;font-weight:bold;color:#005682;}
.deqh li.q1{line-height:33px;background:#fff url(../images/deta_qh.gif) no-repeat;}
.deqh li a{width:82px;height:33px;float:left;line-height:30px;font-size:14px;font-weight:bold;color:#353535;}
.delc{width:100%;height:29px;float:left;background:#C8D3D9 url(../images/deta_ti.gif) -0 -41px;line-height:29px;}
.delc .left{margin-left:22px;}
.delc .right{margin-right:15px;}
.dewt{width:100%;float:left;}
.dewt p{padding:5px;line-height:20px;text-align:left;}
.dewt .jt{width:320px;margin:auto;}
.orange{color:#FF3300;}
.dewt .jt .hidd{float:left;}
.dewt .jt .hidd img{width:256px;height:120px;margin:auto;}

.dewt .bb{width:100%;float:left;background:#FAFAFA;margin-top:5px;}
.dewt .bb p{padding:5px;color:#505050;}

.dely{width:100%;float:left;background:url(../images/type_bg.gif);padding:5px 0;line-height:20px;}
.dely .left{margin-left:10px;text-align:left;}
.dely .right{margin-right:10px;text-align:right;}
.iconmy{width:18px; height:18px;float:left;background:url(../images/icon_my.gif) no-repeat;}

.delist{width:100%;float:left;background:url(../images/type_bg.gif);padding:10px 0;line-height:20px;}
.delist .jz{width:49%;float:left;}
.iconhot{width:134px;height:34px;background:url(../images/list_x.gif) no-repeat;font-weight:bold;color:#3D3D3D;font-size:18px;text-align:left;padding-left:18px;margin-bottom:auto}
.iconnew{width:134px;height:34px;background:url(../images/list_y.gif) no-repeat;font-weight:bold;color:#C64600;font-size:18px;text-align:left;padding-left:18px;margin-bottom:auto}

.logoscroll {
 width:280px;
 height: 420px;
 margin-left:auto;
 margin-right:auto;
 margin-top: 0px;
 overflow: hidden;
 position: relative;
 border: 1px;
}
 .button .left{
 margin-right:0px;margin-left:-160px;margin-top:240px;width:0px;text-align:center;padding-top: 0px;
 position: absolute;
 padding-right: 0px;
 }
 .button .right{
 margin-right:0px;margin-left:150px;margin-top:240px;width:0px;text-align:center;padding-top: 0px;
 position: absolute;
 padding-left: 0px;
 }
 .logoscroll ul li {
 text-align: center;
 line-height: 0px;
 position: absolute;
 font-size: 40px;
 font-weight: bold;
}
 .logoscroll ul li img{
 height: 420px;
 width: 280px;
}


/*评论*/
.deping{width:100%;height:36px;float:left;background:#CCCCCC url(../images/deta_ti.gif) -0 -73px;padding-top:8px;}
.deping .pan{width:64px;height:27px;background:#CCCCCC url(../images/de_p.gif) no-repeat;color:#fff;font-weight:bold;}
.deping .psr{width:225px;height:23px;border:1px #7F9DB9 solid;}
.depw{width:100%;float:left;}
.depw p{padding:5px;line-height:20px;text-align:left;}
.depw p.p1{border-bottom:1px #C8D3D9 solid}
.depw p.p2{border-top:1px #fff solid;background:#F2F2F2;}
.depw p .name{color:#7AA5B9;margin-left:5px;}
.depw p .time{color:#929292;margin-left:5px;}

/*积分*/
.dejme{width:100%;float:left;}
.dejme dl{width:100%;float:left;}
.dejme dt{width:100%;height:30px;line-height:30px;float:left;background:#CCCCCC url(../images/deta_ti.gif) -0 -118px;color:#828282;}
.dejme dt .dt1{width:20%;float:left;}
.dejme dt .dt2{width:59%;float:left;}
.dejme dt .dt3{width:20%;float:right;}

.dejme dd{width:100%;float:left;line-height:30px;}
.dejme dd .dd1{width:20%;float:left;color:#000;}
.dejme dd .dd2{width:59%;float:left;color:#0078B6;}
.dejme dd .dd3{width:20%;float:right;color:#FF9F26;}

.dejf{width:100%;float:left;}
.dejf dl{width:100%;float:left;}
.dejf dt{width:100%;height:30px;line-height:30px;float:left;background:#CCCCCC url(../images/deta_ti.gif) -0 -149px;color:#828282;}
.dejf dt .dt1{width:20%;float:left;}
.dejf dt .dt2{width:59%;float:left;}
.dejf dt .dt3{width:20%;float:right;}

.dejf dd{width:100%;float:left;line-height:30px;background:#F2F2F2;}
.dejf dd.b1{border-bottom:1px #C8D3D9 solid;border-top:1px #fff solid;}
.dejf dd.b2{border-bottom:1px #C8D3D9 solid;border-top:1px #fff solid;}
.dejf dd{width:100%;float:left;line-height:30px;}
.dejf dd .dd1{width:20%;float:left;color:#000;}
.dejf dd .dd2{width:59%;float:left;color:#0078B6;}
.dejf dd .dd3{width:20%;float:right;color:#FF9F26;}

/*分类*/
.type{width:100%;float:left;}
.type ul{width:100%;float:left;}
.type li{width:100%;float:left;height:60px;background:url(../images/type_bg.gif);padding:10px 0;}
.type li.tytop{width:100%;float:left;height:60px;background:url(../images/type_bg.gif) -0 -80px;}
.type li .pop{width:160px;float:left;text-align:left;}
.type li .pop .name{width:160px;float:left;color:#000;font-size:21px;font-weight:bold;}
.type li .pop .name a{color:#000;}
.type .pe{width:160px;float:left;height:23px;text-align:left;margin-top:10px;position:relative;}
.type .pe1{width:8px;height:23px;float:left;background:url(../images/type_bg.gif) -0 -191px;}
.type .pe2{height:23px;float:left;line-height:23px;background:url(../images/type_bg.gif) -0 -160px;}
.type .pe3{width:6px;height:23px;float:left;background:url(../images/type_bg.gif) no-repeat -49px -191px;}
.type .tup{float:left;width:60px;height:60px;margin:0 10px;}
.type .tup img{width:60px;height:60px;}
.type .jr{width:25px;float:right;margin:20px 10px 0 0;}
.type em{width:10px;height:7px;overflow:hidden;left:20px;top:-6px;z-index:5;position:absolute;background:url(../images/type_bg.gif) no-repeat -21px -185px;}

/*登陆*/
.login{width:100%;float:left;background:#FAFAFA;padding:10px 0;}
.login0{width:100%;float:left;font-size:14px;}
.login1{width:24%;float:left;text-align:right;line-height:30px;padding-top:10px;}
.login2{width:76%;float:left;text-align:left;padding-top:10px;line-height:22px;color:#515151;}
.login2 .ku{width:170px;height:22px;background:url(../images/log_bg.gif) no-repeat;padding:5px;line-height:22px;}
.login2 .an{width:82px;height:30px;background:url(../images/login.gif) no-repeat;line-height:22px;color:#fff;font-weight:bold;margin:7px 0;}
.login p{width:100%;float:left;line-height:30px;padding-top:10px;text-align:right; color:#515151;}

.help{width:100%;float:left;background:#FCFCFC;}
.help p{padding:5px;line-height:20px;text-align:left;}
.help h2{text-align:left;}

/*星级*/
.starbasic{width:70px;height:14px;margin:auto;float:left;background: url(../images/star.gif) no-repeat;overflow:hidden;margin-top:2px;}
.star5 {background-position: 0 0;}
.star4 {background-position: -14px 0;}
.star3 {background-position: -28px 0;}
.star2 {background-position: -42px 0;}
.star1 {background-position: -56px 0;}
.star0 {background-position: -70px 0;}

.bottom{width:100%;background:#767676;float:left;margin:0 auto;padding:15px 0 15px 0px;line-height:22px;text-align:center;}
.bottom ul li a{color:#fff;padding-right:10px;} 
#footer{width:100%;float:left;padding:5px 0;text-align:center;line-height:25px;color:#7B7B7B;}
#footer a{color:#000;margin:0 10px;}