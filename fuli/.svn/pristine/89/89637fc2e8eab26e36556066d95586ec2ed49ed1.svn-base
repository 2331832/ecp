<#include "../inc/layout.html"> <@layout title="用户">
<div class="nav">
	<ul class="cc">
		<li class="current">酒帮分析</li>
	</ul>
</div>
<div class="h_a">搜索</div>
<form method="post"
	action="${root!}/jiubang/list?qryField=ctime$le,ctime$ge">
	<div class="search_type cc mb10">
		<div class="mb10">
			<span class="mr20">时间： <input type="text" name="ctime$ge"
				class="input length_2 J_date date"
				value="${(request.getParameter('ctime$ge'))!}" style="width: 80px;"
				autocomplete="off">- <input type="text"
				class="input length_2 J_date date" name="ctime$le"
				value="${(request.getParameter('ctime$le'))!}" style="width: 80px;"
				autocomplete="off"> &nbsp; &nbsp;关键字： <input type="text"
				class="input length_2" name="key" style="width: 200px;" value="${key!}"
				placeholder="请输入关键字..."><input type="submit" class="btn"
				value="搜索"></span>
		</div>
	</div>
</form>
<div class="common-form">
	<form method="post" class="J_ajaxForm" action="#">
		<div class="table_list">
			<table width="100%">
				<thead>
					<tr>
						<td rowspan="2">ID</td>
						<td rowspan="2">酒帮名称</td>
						<td rowspan="2">创建时间</td>
						<td colspan="9" align="center">帮主</td>
						<td rowspan="2">操作</td>
					</tr>
					<tr>
						<td>姓名</td>
						<td>电话</td>
						<td>昵称</td>
						<td>头像</td>
						<td>性别</td>
						<td>省</td>
						<td>市</td>
					</tr>
				</thead>
				<tbody>
					<#list page.list as u>
					<tr>
						<td>${(u.id)!}</td>
						<td>${(u.jbname)!}</td>
						<td>${(u.ctime)!}</td>
						<td>${(u.uname)!}</td>
						<td>${(u.nickname)!}</td>
						<td align='center'><img src="${(u.headimgurl)!}" height="40px;"></td>
						<td>${sex[(u.sex)!0]}</td>
						<td>${TextUtil.fromInt(u.subscribe_time)?string("yyyy-MM-dd HH:mm:ss")}</td>
						<td>${(u.province)!}</td>
						<td>${(u.city)!}</td>
						<td class="text-c"><a href="javascript:open_iframe_dialog('${root!}/jbmember/list?qryField=jbid&jbid=${u.id}','帮会成员');">帮会成员</a>
					</tr>
					</#list>
				</tbody>
			</table>
			<div class="p10"><@paginate action="/jiubang/list" currentPage=pageNo totalPage=page.totalPage!0	totalCount=page.totalCount!0/></div>
		</div>
	</form>
</div>
</@layout>
