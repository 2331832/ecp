<#include "../inc/layout.html"> <@layout title="查看微信用户">
<div class="nav">
	<ul class="cc">
		<li class="current"><a href="#">查看微信用户</a></li>
	</ul>
</div>
<div class="common-form">
	<div class="table_list">
		<table cellpadding="0" cellspacing="0" class="table_form" width="100%">
			<tbody>
				<tr>
					<th width="100px">微信昵称:</th>
					<td width="300px">${(po.nickname)!}</td>
					<th width="100px">头像:</th>
					<td width="300px"><img src="${(po.headimgurl)!}" height="35px;"></td>
				</tr>
				<tr>
					<th width="100px">姓名:</th>
					<td width="300px">${(po.uname)!}</td>
					<th width="100px">手机:</th>
					<td width="300px">${(po.phone)!}</td>
				</tr>
				<tr>
					<th width="100px">性别:</th>
					<td width="300px">${sex[(po.sex)!0]}</td>
					<th width="100px">酒帮类型:</th>
					<td width="300px">${jbm[(po.jbm)!0]}</td>
				</tr>
				<tr>
					<th width="100px">购买次数:</th>
					<td width="300px">${(po.shopcount)!0}</td>
					<th width="100px">消费合计:</th>
					<td width="300px">${(po.amt)!}</td>
				</tr>
				<tr>
					<th width="100px">收货地址:</th>
					<td width="300px">${(po.addr)!}</td>
					<th width="100px">开户名:</th>
					<td width="300px">${(po.danwei)!}</td>
				</tr>
				<tr>
					<th width="100px">开户银行:</th>
					<td width="300px">${(po.bank)!}</td>
					<th width="100px">银行账号:</th>
					<td width="300px">${(po.account)!}</td>
				</tr>
			</tbody>
		</table>
	</div>
</div>
</@layout>
