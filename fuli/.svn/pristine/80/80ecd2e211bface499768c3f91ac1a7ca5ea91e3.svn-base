package net.loyin.util.wechat.bean;

public class WcRecorder implements java.io.Serializable {
	/** 描述 */
	private static final long serialVersionUID = 6559045743316482299L;
	private Integer recorderId;
	private String userNo;
	private String fromUserName;
	private Long createTime;
	private String conts; // 内容
	private String flag; // y为订阅，n为取消订阅，o为正常消息

	public WcRecorder() {
	}

	public WcRecorder(String userNo, String fromUserName, Long createTime, String conts, String flag) {
		super();
		this.userNo = userNo;
		this.fromUserName = fromUserName;
		this.createTime = createTime;
		this.conts = conts;
		this.flag = flag;
	}

	public Integer getRecorderId() {
		return recorderId;
	}

	public void setRecorderId(Integer recorderId) {
		this.recorderId = recorderId;
	}

	public String getUserNo() {
		return userNo;
	}

	public void setUserNo(String userNo) {
		this.userNo = userNo;
	}

	public String getFromUserName() {
		return fromUserName;
	}

	public void setFromUserName(String fromUserName) {
		this.fromUserName = fromUserName;
	}

	public Long getCreateTime() {
		return createTime;
	}

	public void setCreateTime(Long createTime) {
		this.createTime = createTime;
	}

	public String getConts() {
		return conts;
	}

	public void setConts(String conts) {
		this.conts = conts;
	}

	public String getFlag() {
		return flag;
	}

	public void setFlag(String flag) {
		this.flag = flag;
	}
}
