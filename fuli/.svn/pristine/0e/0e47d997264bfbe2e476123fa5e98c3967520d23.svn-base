package net.loyin.util.wechat.bean;

public class WcTreeMenber implements java.io.Serializable {
	private static final long serialVersionUID = 4258938550648246713L;
	private int mid;
	private String fromUserName;
	private int useflow;
	private int usemanure;
	private String friendids;

	public WcTreeMenber() {
	}

	public WcTreeMenber(String fromUserName, int useflow, int usemanure, String friendids) {
		this.fromUserName = fromUserName;
		this.useflow = useflow;
		this.usemanure = usemanure;
		this.friendids = friendids;
	}

	public int getMid() {
		return mid;
	}

	public void setMid(int mid) {
		this.mid = mid;
	}

	public String getFromUserName() {
		return fromUserName;
	}

	public void setFromUserName(String fromUserName) {
		this.fromUserName = fromUserName;
	}

	public int getUseflow() {
		return useflow;
	}

	public void setUseflow(int useflow) {
		this.useflow = useflow;
	}

	public int getUsemanure() {
		return usemanure;
	}

	public void setUsemanure(int usemanure) {
		this.usemanure = usemanure;
	}

	public String getFriendids() {
		return friendids;
	}

	public void setFriendids(String friendids) {
		this.friendids = friendids;
	}

}
